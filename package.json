{
  "name": "inet-rest-api",
  "version": "1.0.0",
  "description": "INET RESTful API with NestJS (TypeScript), MySQL, TypeORM, WebSocket, BullMQ, and Elasticsearch",
  "private": true,
  "scripts": {
    "dev": "./scripts/dev-db.sh && NODE_ENV=development nest start --watch",
    "dev:clear": "./scripts/dev-db-clear.sh",
    "prod": "NODE_ENV=production node dist/src/main",
    "build": "rimraf dist && nest build",
    "ci:prettier": "prettier --check \"{**/*,*}.{ts,js,json}\"",
    "ci:eslint": "eslint \"**/*.{ts,js}\"",
    "ci:jest": "jest",
    "ci:jest:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-multer": "^2.0.2",
    "fastify-swagger": "^5.1.0",
    "form-data": "^4.0.0",
    "jsdom": "^19.0.0",
    "mysql2": "^2.3.3",
    "node-fetch": "^2.6.7",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.44"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.6.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
